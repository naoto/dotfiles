set tabstop=2
set shiftwidth=2
set expandtab
set nocompatible
set number

syntax on
filetype on
filetype indent on
filetype plugin on

let skk_jisyo = '~/.skk-jisyo'
let skk_large_jisyo = '~/.vim/skk_jisyo/SKK-JISYO.L'
let skk_auto_save_jisyo = 1
let skk_keep_state = 0
let skk_egg_like_newline = 1
let skk_show_annotation = 1
let skk_use_face = 1

let g:neocomplcache_enable_at_startup = 1
